@article{Ouyang.2020.J.Chem.Phys..153.134202,
  title = {Nonlinear Fluorescence Spectroscopy of Layered Perovskite Quantum Wells},
  author = {Ouyang, Zhenyu and Zhou, Ninghao and Hu, Jun and Williams, Olivia F. and Yan, Liang and You, Wei and Moran, Andrew M.},
  year = {2020},
