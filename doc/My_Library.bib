@article{Rivalta.2014.J.Phys.Chem.B.118.8396,
  title = {Modelling {{Time-Resolved Two-Dimensional Electronic Spectroscopy}} of the {{Primary Photoisomerization Event}} in {{Rhodopsin}}},
