@article{Bondarenko.2020.Chem.Phys..529.110478,
  title = {Comparison of Methods to Study Excitation Energy Transfer in Molecular Multichromophoric Systems},
  author = {Bondarenko, Anna S. and Knoester, Jasper and Jansen, Thomas L.C.},
  year = {2020},
