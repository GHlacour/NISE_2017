@article{Mattoni.2015.J.Phys.Chem.C.119.17421,
  title = {Methylammonium {{Rotational Dynamics}} in {{Lead Halide Perovskite}} by {{Classical Molecular Dynamics}}: {{The Role}} of {{Temperature}}},
  shorttitle = {Methylammonium {{Rotational Dynamics}} in {{Lead Halide Perovskite}} by {{Classical Molecular Dynamics}}},
  author = {Mattoni, A. and Filippetti, A. and Saba, M. I. and Delugas, P.},
