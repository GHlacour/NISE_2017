@article{Farag.2016.J.Phys.Chem.Lett..7.3328,
  title = {Probing the {{Interstate Coupling}} near a {{Conical Intersection}} by {{Optical Spectroscopy}}},
  author = {Farag, Marwa H. and Jansen, Thomas L. C. and Knoester, Jasper},
  year = {2016},
