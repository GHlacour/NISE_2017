}

@article{Tempelaar.2016.J.Phys.Chem.A.120.3042,
  title = {Laser-{{Limited Signatures}} of {{Quantum Coherence}}},
  author = {Tempelaar, Roel and Halpin, Alexei and Johnson, Philip J. M. and Cai, Jianxin and Murphy, R. Scott and Knoester, Jasper and Miller, R. J. Dwayne and Jansen, Thomas L. C.},
  year = {2016},
