@article{Akhtar.2019.J.Phys.Chem.B.123.1090,
  title = {Anisotropic {{Circular Dichroism}} of {{Light-Harvesting Complex II}} in {{Oriented Lipid Bilayers}}: {{Theory Meets Experiment}}},
  shorttitle = {Anisotropic {{Circular Dichroism}} of {{Light-Harvesting Complex II}} in {{Oriented Lipid Bilayers}}},
  author = {Akhtar, Parveen and Lindorfer, Dominik and Lingvay, M{\'o}nika and Pawlak, Krzysztof and Zsiros, Ott{\'o} and Siligardi, Giuliano and J{\'a}vorfi, Tam{\'a}s and Dorogi, M{\'a}rta and Ughy, Bettina and Garab, Gy{\H o}z{\H o} and Renger, Thomas and Lambrev, Petar H.},
  year = {2019},
