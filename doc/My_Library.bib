@article{Jansen.2019.J.Chem.Phys..150.100901,
  title = {Theory of Coherent Two-Dimensional Vibrational Spectroscopy},
  author = {Jansen, Thomas L. C. and Saito, Shinji and Jeon, Jonggu and Cho, Minhaeng},
  year = {2019},
