@article{Halavaty.2007.Biochem..46.14001,
  title = {N- and {{C-Terminal Flanking Regions Modulate Light-Induced Signal Transduction}} in the {{LOV2 Domain}} of the {{Blue Light Sensor Phototropin}} 1 from {{{\emph{Avena}}}}{\emph{ Sativa}} {\textsuperscript{,}}},
