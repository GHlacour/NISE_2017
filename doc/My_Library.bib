@article{Farag.2018.Phys.Chem.Chem.Phys..20.12746,
  title = {The Origin of Absorptive Features in the Two-Dimensional Electronic Spectra of Rhodopsin},
  author = {Farag, Marwa H. and Jansen, Thomas L. C. and Knoester, Jasper},
  year = {2018},
  journal = {Phys. Chem. Chem. Phys.},
