@article{Tempelaar.2017.J.Phys.Chem.Lett..8.6113,
  title = {Exciton\textendash{{Exciton Annihilation Is Coherently Suppressed}} in {{H-Aggregates}}, but {{Not}} in {{J-Aggregates}}},
