@article{Do.2019.J.Chem.Phys..151.205101,
  title = {Revealing the Excitation Energy Transfer Network of {{Light-Harvesting Complex II}} by a Phenomenological Analysis of Two-Dimensional Electronic Spectra at 77 {{K}}},
  author = {Do, Thanh Nhut and {Huerta-Viga}, Adriana and Akhtar, Parveen and Nguyen, Hoang Long and Nowakowski, Pawe{\l} J. and Khyasudeen, M. Faisal and Lambrev, Petar H. and Tan, Howe-Siang},
