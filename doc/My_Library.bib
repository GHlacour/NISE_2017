@article{Baiz.2011.J.Phys.Chem.B.115.5322,
  title = {Molecular {{Theory}} and {{Simulation}} of {{Coherence Transfer}} in {{Metal Carbonyls}} and {{Its Signature}} on {{Multidimensional Infrared Spectra}}},
  author = {Baiz, Carlos R. and Kubarych, Kevin J. and Geva, Eitan},
  year = {2011},
