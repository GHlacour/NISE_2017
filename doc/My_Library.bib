@article{Ratsep.2007.J.Lumin..127.251,
  title = {Electron\textendash Phonon and Vibronic Couplings in the {{FMO}} Bacteriochlorophyll a Antenna Complex Studied by Difference Fluorescence Line Narrowing},
  author = {R{\"a}tsep, Margus and Freiberg, Arvi},
  year = {2007},
  month = nov,
  journal = {J. Lumin.},
  volume = {127},
  number = {1},
  pages = {251--259},
  issn = {00222313},
  doi = {10.1016/j.jlumin.2007.02.053},
  abstract = {The electron\textendash phonon and vibronic couplings governing the spectral properties have been studied in the Fenna\textendash Matthews\textendash Olson (FMO) bacteriochlorophyll a (BChl a)\textendash protein complex at 4.5 K using a spectrally selective difference fluorescence line-narrowing technique. The complex is a part of the light-harvesting system of green photosynthetic bacteria. Its lowest-energy absorption band, peaking at 826 nm and responsible for the fluorescence, is believed to be due to Qy transitions of largely isolated molecules. One of the main merits of the used method compared with the more common fluorescence line narrowing is that the zero-phonon lines (ZPL) resonant with the excitation laser can be accurately measured, allowing precise determination of the Huang\textendash Rhys (HR) factors, the main characteristics of the linear electron\textendash phonon and vibronic coupling strengths. Over 60 individual vibrational modes of intra- and intermolecular origin have been resolved in the energy range of 45\textendash 1600 cm\`A1. The small HR factors for these modes, ranging between 0.001 and 0.018, add up to a value of Svib {$\frac{1}{4}$} 0.3870.07. The effective HR factor for the phonons, Sph, was found clearly wavelengthdependent, varying from \$0.7 at short wavelengths to \$0.3 at the long-wavelength tail of the absorption spectrum. Coupling between the BChl a molecules is likely responsible for this wavelength dependence.},
  langid = {english},
  file = {/Users/lacour/Zotero/storage/TCJTDKIR/Rätsep and Freiberg - 2007 - Electron–phonon and vibronic couplings in the FMO .pdf}
}

@article{Raul.2020.SciRep.10.21198,
  title = {Excited State Dynamics and Exciton Diffusion in Triphenylamine/Dicyanovinyl Push\textendash Pull Small Molecule for Organic Optoelectronics},
  author = {Raul, Benedito A. L. and Luponosov, Yuriy N. and Yang, Wenyan and Surin, Nikolay M. and Douh{\'e}ret, Olivier and Min, Jie and Jansen, Thomas L. C. and Ponomarenko, Sergei A. and Pshenichnikov, Maxim S.},
  year = {2020},
  month = dec,
  journal = {Sci Rep},
  volume = {10},
  number = {1},
  pages = {21198},
  issn = {2045-2322},
  doi = {10.1038/s41598-020-78197-2},
  abstract = {Abstract                            Triphenylamine-based small push\textendash pull molecules have recently attracted substantial research attention due to their unique optoelectronic properties. Here, we investigate the excited state de-excitation dynamics and exciton diffusion in TPA-T-DCV-Ph-F small molecule, having simple chemical structure with asymmetrical architecture and end-capped with electron-withdrawing               p               -fluorodicyanovinyl group. The excited state lifetime in diluted solutions (0.04~ns in toluene and 0.4~ns in chloroform) are found to be surprisingly shorter compared to the solid state (3~ns in PMMA matrix). Time-dependent density functional theory indicates that this behavior originates from non-radiative relaxation of the excited state through a conical intersection between the ground and singlet excited state potential energy surfaces. Exciton diffusion length of\,\textasciitilde\,16~nm in solution processed films was retrieved by employing time-resolved photoluminescence volume quenching measurements with Monte Carlo simulations. As means of investigating the device performance of TPA-T-DCV-Ph-F, we manufactured solution and vacuum processed bulk heterojunction solar cells that yielded efficiencies of\,\textasciitilde\,1.5\% and\,\textasciitilde\,3.7\%, respectively. Our findings demonstrate that the short lifetime in solutions does not hinder per se long exciton diffusion length in films thereby granting applications of TPA-T-DCV-Ph-F and similar push\textendash pull molecules in vacuum and solution processable devices.},
  langid = {english},
  file = {/Users/lacour/Zotero/storage/J8DAL8LF/Raul et al. - 2020 - Excited state dynamics and exciton diffusion in tr.pdf}
}

