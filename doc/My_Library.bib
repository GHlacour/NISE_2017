  author = {Curutchet, Carles and Novoderezhkin, Vladimir I. and Kongsted, Jacob and {Mu{\~n}oz-Losa}, Aurora and {van Grondelle}, Rienk and Scholes, Gregory D. and Mennucci, Benedetta},
  year = {2013},
