@article{Kratochvil.2017.J.Am.Chem.Soc..139.8837,
  title = {Probing the {{Effects}} of {{Gating}} on the {{Ion Occupancy}} of the {{K}} {\textsuperscript{+}} {{Channel Selectivity Filter Using Two-Dimensional Infrared Spectroscopy}}},
  author = {Kratochvil, Huong T. and Maj, Micha{\l} and Matulef, Kimberly and Annen, Alvin W. and Ostmeyer, Jared and Perozo, Eduardo and Roux, Beno{\^i}t and Valiyaveetil, Francis I. and Zanni, Martin T.},
  year = {2017},
